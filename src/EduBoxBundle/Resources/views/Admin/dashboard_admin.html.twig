{% extends '@EduBox/admin_layout.html.twig' %}

{% set title %}
    Admin Dashboard
{% endset %}

{% set breadcrumbs = [{'label':'Test1'},{'label':'Test2'}] %}

{% block content %}
    {% if is_granted('ROLE_ADMIN') %}
        <meta http-equiv="refresh" content="0;url={{ path('edubox.admin.organisation_list') }}">
    {% elseif is_granted('ROLE_TEACHER') %}
        <meta http-equiv="refresh" content="0;url={{ path('edubox.admin.subject_schedule_list') }}">
    {% elseif is_granted('ROLE_STUDENT') %}
        <meta http-equiv="refresh" content="0;url={{ path('edubox.admin.diary_list') }}">
    {% elseif is_granted('ROLE_PARENT') %}
        <meta http-equiv="refresh" content="0;url={{ path('edubox.admin.mark_list') }}">
    {% endif %}
{% endblock %}
